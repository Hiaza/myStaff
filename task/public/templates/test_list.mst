{{^err}}{{#isSearch}} <p >Found: {{numOfRes}} results </p> {{/isSearch}} {{/err}}

{{^err}}<table align="center" id="table_users" style="margin-top:30px">
    <tr>
        <th>Test Name</th>
        <th>Created</th>
    </tr>

    {{#all_items}}
        <tr>
            <td><a href="/tests/{{_id}}">{{name}}</a></td>
            <td> <time>{{created}}</time> </td>
        </tr>
    {{/all_items}}
{{/err}}</table>

<h1>{{err}}</h1>